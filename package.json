{
  "name": "@infermedica/component-library",
  "description": "Vue 3 UI library for healthcare and not only.",
  "version": "0.5.2",
  "engines": {
    "node": ">=16.13.2",
    "pnpm": ">=6.32"
  },
  "license": "MIT",
  "scripts": {
    "test:generate-output": "vitest --outputFile=.unit-test-results.json --run || true",
    "test:unit": "vitest --run",
    "test:watch": "vitest -w --changed",
    "lint": "eslint --fix --ext .js,.vue,.ts src",
    "check:types": "vue-tsc --noEmit",
    "stylelint": "stylelint src/**/*.{vue,scss,css} --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "create:index": "node scripts/create-index.js",
    "create:component": "node scripts/create-component.js",
    "create:icons-list": "node scripts/create-icons-list.js",
    "update:docs": "node scripts/update-components-docs.mjs && pnpm lint",
    "chromatic": "chromatic --exit-zero-on-changes",
    "serve": "vite --open",
    "preview": "vite preview --open --port 8080",
    "build": "vite build --out-dir dist",
    "husky": "husky install && chmod ug+x .husky/*"
  },
  "main": "index.ts",
  "module": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./src/": "./src/",
    "./styles/": "./src/styles/",
    "./breakpoints": "./src/styles/exports/breakpoints.module.scss",
    "./mixins/": "./src/styles/mixins",
    "./atoms/": "./src/components/atoms/",
    "./molecules/": "./src/components/molecules/",
    "./organisms/": "./src/components/organisms/",
    "./templates/": "./src/components/templates/",
    "./directives/": "./src/utilities/directives/",
    "./helpers/": "./src/utilities/helpers/",
    "./composable/": "./src/composable/",
    "./icons/": "./src/assets/icons/"
  },
  "files": [
    "src/*",
    "index.ts",
    "scripts/create-index.js"
  ],
  "dependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "core-js": "^3.23.5",
    "date-fns": "^2.28.0",
    "fast-deep-equal": "^3.1.3",
    "uid": "^2.0.0",
    "vite": "^3.0.2",
    "vite-svg-loader": "^3.4.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@brightlayer-ui/storybook-rtl-addon": "^1.1.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-backgrounds": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-jest": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-measure": "^6.5.12",
    "@storybook/addon-outline": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/channel-postmessage": "^6.5.12",
    "@storybook/channel-websocket": "^6.5.12",
    "@storybook/client-api": "^6.5.12",
    "@storybook/client-logger": "^6.5.12",
    "@storybook/jest": "^0.0.10",
    "@storybook/preview-web": "^6.5.12",
    "@storybook/test-runner": "^0.8.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.12",
    "@storybook/vue3": "^6.5.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitest/ui": "^0.23.4",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.5.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-newlines": "^1.2.3",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vitest-globals": "^1.1.0",
    "eslint-plugin-vue": "^9.2.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "file-save": "^0.2.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prompts": "^2.4.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-string-replace": "^1.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.32.7",
    "sass-loader": "^13.0.2",
    "storybook-addon-designs": "^6.3.1",
    "stringify-object": "^4.0.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^26.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.7.4",
    "uppercamelcase": "^3.0.0",
    "vitest": "^0.18.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.13"
  }
}
