{
  "name": "@infermedica/component-library",
  "description": "Vue 3 UI library for healthcare and not only.",
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "test:generate-output": "vue-cli-service test:unit --json --outputFile=.jest-test-results.json || true",
    "test:unit": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch --onlyChanged",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "stylelint": "stylelint src/**/*.{vue,scss,css} --fix",
    "prestorybook": "npm run create:icons-list && npm run test:generate-output",
    "storybook": "start-storybook -s ./public -p 6006",
    "prebuild-storybook": "npm run create:icons-list && npm run test:generate-output",
    "build-storybook": "build-storybook -s ./public",
    "create:index": "node scripts/create-index.js",
    "create:component": "node scripts/create-component.js",
    "create:icons-list": "node scripts/create-icons-list.js",
    "update:docs": "node scripts/update-components-docs.mjs && npm run lint",
    "postinstall": "npm run create:index",
    "chromatic": "chromatic --exit-zero-on-changes",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "main": "index.js",
  "module": "index.js",
  "exports": {
    ".": "./index.js",
    "./src/": "./src/",
    "./styles/": "./src/styles/",
    "./breakpoints": "./src/styles/exports/breakingpoints.scss",
    "./atoms/": "./src/components/atoms/",
    "./molecules/": "./src/components/molecules/",
    "./organisms/": "./src/components/organisms/",
    "./templates/": "./src/components/templates/",
    "./directives/": "./src/utilities/directives/",
    "./helpers/": "./src/utilities/helpers/",
    "./composable/": "./src/composable/",
    "./icons/": "./src/assets/icons/"
  },
  "files": [
    "src/*",
    "index.js",
    "scripts/create-index.js"
  ],
  "dependencies": {
    "core-js": "^3.8.3",
    "date-fns": "^2.28.0",
    "fast-deep-equal": "^3.1.3",
    "uid": "^2.0.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@brightlayer-ui/storybook-rtl-addon": "^1.1.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@ljcl/storybook-addon-cssprops": "^2.3.2",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-jest": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue3": "^6.5.6",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.5.4",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^8.0.3",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "file-save": "^0.2.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prompts": "^2.4.2",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "storybook-addon-designs": "^6.2.1",
    "stringify-object": "^4.0.1",
    "stylelint": "^14.5.3",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.0.4",
    "typescript": "~4.5.5",
    "uppercamelcase": "^3.0.0",
    "vue-loader": "^16.8.3",
    "vue-svg-loader": "^0.17.0-beta.2"
  }
}
